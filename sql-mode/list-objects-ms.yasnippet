select
    (sc.name + '.' + ob.name) objeto,
    ob.type
from sys.objects ob
inner join  sys.schemas sc on ob.schema_id = sc.schema_id
where (ob.type in ('U', 'V', 'P'))
order by ob.type, sc.name, ob.name
