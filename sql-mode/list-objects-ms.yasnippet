select
    substring(us.name + '.' + ob.name, 0, 50) objeto,
    ob.type tipo
from sysobjects ob
left join sysusers us on (ob.uid = us.uid)
where (ob.type in ('U', 'V', 'P'))
order by ob.type, ob.name
