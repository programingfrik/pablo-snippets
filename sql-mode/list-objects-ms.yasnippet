select
    (TABLE_SCHEMA + '.' + TABLE_NAME) objeto,
    TABLE_TYPE tipo
from INFORMATION_SCHEMA.TABLES
union all
select
    (SPECIFIC_SCHEMA + '.' + ROUTINE_NAME) objeto,
    ROUTINE_TYPE tipo
from INFORMATION_SCHEMA.ROUTINES
order by tipo, objeto
